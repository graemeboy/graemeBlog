    <script>
    jQuery(document).ready(function(e) {
    	$('#btn-read-articles').click(function (e) {
    		e.preventDefault();
    		$('.home-cat-container-first').hide();
    		$('.home-cat-container-second').show();

    		$('#header-sub').text("Choose a category that interests you below:");
    		//$('.home-actions').hide();
    	});
        setTimeout(function() {
            e(".home-based-text").fadeIn(400);
            e(".home-based-twitter").fadeIn(1e3)
        }, 1e3);
        var t;
        e("#creative-header").fadeIn(500);
        t = 1;
        e(".home-vis-stat").each(function() {
            e(this).fadeIn(450 * t);
            t++
        });
        t = 1;
        e(".home-cat").each(function() {
            e(this).fadeIn(t * 250);
            t++
        });
        t = 1;
        e(".home-soc-btn").each(function() {
            e(this).fadeIn(t * 100);
            t++
        })
    })
    </script>
