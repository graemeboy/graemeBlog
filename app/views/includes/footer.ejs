
        <!-- Clear before adding the footer -->
        <div class="clearfix"></div>
        <footer id="footer">
            <div class="col-sm-12">
                <div class="container">
                    <p>Graeme Boy (Grinnell, IA, USA) - 2014 </p>
                    <p class="footer-credit"></p>
                </div>
            </div>
        </footer>
        

        <% if (typeof category != 'undefined' && category !== 'none' && typeof tags != 'undefined') { %>
          <script>var y = document.getElementsByClassName('page-content'),
              aNode = y[0],
              callAction = document.createElement('div'),
              hashtags = '';
            callAction.className = 'call-to-action';

            var tags = '<%= tags %>';
            if (tags != '') {
              hashtags = '&hashtags=' + tags.replace('.', '');
            }
            console.log(hashtags);
            callAction.innerHTML = '<strong>If you enjoyed this post or have something to say about it, <a href="http://twitter.com/share?text=Hey @graeme_boy, I just read your post on <%- title.toLowerCase() %>' + hashtags + '">send me a tweet.</a></strong>';
            aNode.appendChild(callAction);
          </script>
        <% } %>

        <% if (typeof scripts != 'undefined') { 
            for (var i = 0; i < scripts.length; i++) { %>
                <script src="<%= scripts[i] %>"></script>
            <% } 
        } %>
        <% if (typeof inlineScripts != 'undefined') { for (i = 0; i < inlineScripts.length; i++) { %>
            <script><%= inlineScripts[i] %></script>
        <% } } %>
